{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "npm start --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha 'test/test.js'",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.0.4",
    "@sendgrid/mail": "^7.4.0",
    "aws-sdk": "^2.799.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^1.19.1",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.0.1",
    "socket.io": "^3.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0"
  },
  "engines": {
    "node": "14.2.0"
  }
}
